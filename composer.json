{
    "name": "zvizvi/user-fields",
    "description": "This is my package user-fields",
    "keywords": [
        "zvizvi",
        "laravel",
        "user-fields"
    ],
    "homepage": "https://github.com/zvizvi/filament-user-fields",
    "support": {
        "issues": "https://github.com/zvizvi/filament-user-fields/issues",
        "source": "https://github.com/zvizvi/filament-user-fields"
    },
    "authors": [
        {
            "name": "zvizvi",
            "email": "zvizvi5@outlook.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Zvizvi\\UserFields\\": "src/",
            "Zvizvi\\UserFields\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zvizvi\\UserFields\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zvizvi\\UserFields\\UserFieldsServiceProvider"
            ],
            "aliases": {
                "UserFields": "Zvizvi\\UserFields\\Facades\\UserFields"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
